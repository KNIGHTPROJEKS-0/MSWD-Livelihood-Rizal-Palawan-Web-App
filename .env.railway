# Railway Environment Configuration
# Replace these with your actual Railway service URLs

# Database Configuration (Railway PostgreSQL)
DATABASE_URL=${{Postgres.DATABASE_URL}}
DATABASE_HOST=${{Postgres.PGHOST}}
DATABASE_PORT=${{Postgres.PGPORT}}
DATABASE_NAME=${{Postgres.PGDATABASE}}
DATABASE_USER=${{Postgres.PGUSER}}
DATABASE_PASSWORD=${{Postgres.PGPASSWORD}}

# Redis Configuration (Railway Redis)
REDIS_URL=${{Redis.REDIS_URL}}
REDIS_HOST=${{Redis.REDIS_HOST}}
REDIS_PORT=${{Redis.REDIS_PORT}}

# Application Configuration
ENVIRONMENT=production
DEBUG=false
API_V1_STR=/api/v1
PROJECT_NAME="MSWD Livelihood Rizal Palawan"
PROJECT_VERSION=1.0.0
PORT=${{PORT}}

# CORS Configuration (Update with your Railway domains)
BACKEND_CORS_ORIGINS=["https://mswd-livelihood-web-app-production.up.railway.app","https://your-frontend-domain.railway.app"]

# JWT Configuration
SECRET_KEY=${{SECRET_KEY}}
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Firebase Configuration (Keep your existing values)
VITE_FIREBASE_API_KEY=${{VITE_FIREBASE_API_KEY}}
VITE_FIREBASE_AUTH_DOMAIN=${{VITE_FIREBASE_AUTH_DOMAIN}}
VITE_FIREBASE_PROJECT_ID=${{VITE_FIREBASE_PROJECT_ID}}
VITE_FIREBASE_STORAGE_BUCKET=${{VITE_FIREBASE_STORAGE_BUCKET}}
VITE_FIREBASE_MESSAGING_SENDER_ID=${{VITE_FIREBASE_MESSAGING_SENDER_ID}}
VITE_FIREBASE_APP_ID=${{VITE_FIREBASE_APP_ID}}

# Firebase Backend Configuration
FIREBASE_PROJECT_ID=${{FIREBASE_PROJECT_ID}}
FIREBASE_PRIVATE_KEY=${{FIREBASE_PRIVATE_KEY}}
FIREBASE_CLIENT_EMAIL=${{FIREBASE_CLIENT_EMAIL}}
FIREBASE_CLIENT_ID=${{FIREBASE_CLIENT_ID}}

# N8N Configuration (Railway N8N service)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_EMAIL=${{N8N_BASIC_AUTH_EMAIL}}
N8N_BASIC_AUTH_PASSWORD=${{N8N_BASIC_AUTH_PASSWORD}}
WEBHOOK_URL=${{N8N.RAILWAY_PUBLIC_DOMAIN}}

# Web App Configuration
VITE_API_BASE=https://mswd-livelihood-rizal-palawan-web-app-production.up.railway.app